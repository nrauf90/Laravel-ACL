{
  "name": "nrauf90/laravel-acl",
  "description": "A custom Laravel ACL package that auto-scans controller methods and stores permissions in the database.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "nrauf90",
      "email": "noman.rauf90@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "*"
  },
  "autoload": {
    "psr-4": {
      "Nrauf90\\LaravelAcl\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nrauf90\\LaravelAcl\\LaravelAclServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
